<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.joe.system.vo.SysDictionary" table="sys_dictionary">
        <id name="dictName" column="dict_name">
        </id>
        <property name="dictValue" column="dict_value" />
        <property name="desc" column="description" />
    </class>
</hibernate-mapping>