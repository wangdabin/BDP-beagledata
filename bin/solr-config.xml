<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration>
	<property>
		<name>ha.zookeeper.quorum</name>
		<value>master:2181,master1:2181,node1:2181</value>
	</property>
	<property>
		<name>solr.zookeeper.quorum</name>
		<value>master:2181,master1:2181,node1:2181/solr</value>
	</property>
	<property>
		<name>default.collection</name>
		<value>collection1</value>
	</property>
	<property>
		<name>com.sky.solr.nodes</name>
		<value>master:8983/solr,master1:8983/solr,node1:8983/solr</value>
	</property>
	<property>
		<name>com.sky.httpsolr.use</name>
		<value>true</value>
	</property>
	<property>
		<name>com.sky.httpsolr.url</name>
		<value>http://127.0.0.1:8983/solr</value>
	</property>
</configuration>